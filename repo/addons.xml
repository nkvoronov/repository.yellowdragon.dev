<addon id="skin.confluence-extended-s" version="4.7.5.192" name="Confluence Extended Shortcuts" provider-name="Jezz_X, Team Kodi, YLLOW_DRAGON, plood">
	<requires>
		<import addon="xbmc.gui" version="5.15.0"/>
		<import addon="script.image.resource.select" version="3.0.2"/>
		<import addon="script.skinshortcuts" version="1.1.2"/>
